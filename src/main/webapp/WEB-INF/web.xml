<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>CustomerServlet</servlet-name>
  	<display-name>CustomerServlet</display-name>
  	<description></description>
  	<servlet-class>com.tdn.customercontroller.CustomerServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CustomermangementServlet</servlet-name>
  	<display-name>CustomermangementServlet</display-name>
  	<description></description>
  	<servlet-class>com.tdn.customercontroller.CustomermangementServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CustomerServlet1</servlet-name>
  	<display-name>CustomerServlet1</display-name>
  	<description></description>
  	<servlet-class>com.tdn.customercontroller.CustomerServlet1</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Hello</servlet-name>
  	<display-name>Hello</display-name>
  	<description></description>
  	<servlet-class>com.tdn.customercontroller.Hello</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CustomerServlet</servlet-name>
  	<url-pattern>/CustomerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CustomermangementServlet</servlet-name>
  	<url-pattern>/CustomermangementServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CustomerServlet1</servlet-name>
  	<url-pattern>/CustomerServlet1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Hello</servlet-name>
  	<url-pattern>/Hello</url-pattern>
  </servlet-mapping>
</web-app>
